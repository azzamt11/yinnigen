            "\t\t\t\temit(LoadedAuthState(",
            "\t\t\t\t\tdata: response.data.user,",
            "\t\t\t\t\t));",
            "\t\t\t} else {",
            "\t\t\t\temit(ErrorAuthState(",
            "\t\t\t\t\terror: Error(",
            "\t\t\t\t\t\tmessage: response.message,",
            "\t\t\t\t\t\tisOffline: response.message.toString().toLowerCase().contains(\"socket\")",
            "\t\t\t\t\t),",
            "\t\t\t\t\t",
            "\t\t\t\t));",
            "\t\t\t}",
            "\t\t} catch(e) {",
            "\t\t\temit(ErrorAuthState(",
            "\t\t\t\terror: Error(",
            "\t\t\t\t\tmessage: e.toString(),",
            "\t\t\t\t\tisOffline: e.toString().toLowerCase().contains(\"socket\")",
            "\t\t\t\t),",
            "\t\t\t));",
            "\t\t}",
            "\t}",
            "",
            "\tFuture<void> signUp(email, password, name) async {",
            "\t\temit(LoadingAuthState());",
            "\t\tawait Future.delayed(const Duration(seconds: 5));",
            "\t\ttry {",
            "\t\t\tfinal response = await _repository.signUp({",
            "\t\t\t\t\"email\": email,",
            "\t\t\t\t\"password\": password,",
            "\t\t\t\t\"name\": name",
            "\t\t\t});",
            "\t\t\tif(response.code == 200) {",
            "\t\t\t\temit(LoadedAuthState(",
            "\t\t\t\t\tdata: UserData(id: int.parse(response.data.userId ?? \"0\")),",
            "\t\t\t\t));",
            "\t\t\t} else {",
            "\t\t\t\temit(ErrorAuthState(",
            "\t\t\t\t\terror: Error(",
            "\t\t\t\t\t\tmessage: response.message,",
            "\t\t\t\t\t\tisOffline: response.message.toString().toLowerCase().contains(\"socket\")",
            "\t\t\t\t\t)",
            "\t\t\t\t));",
            "\t\t\t}",
            "\t\t} catch(e) {",
            "\t\t\temit(ErrorAuthState(",
            "\t\t\t\terror: Error(",
            "\t\t\t\t\tmessage: e.toString(),",
            "\t\t\t\t\tisOffline: e.toString().toLowerCase().contains(\"socket\")",
            "\t\t\t\t),",
            "\t\t\t));",
            "\t\t}",
            "\t}",
            "}",
            "",
            "",
            "class Error {",
            "",
            "\tString? message;",
            "\tbool? isOffline;",
