    "yinni-header-interceptor": {
        "prefix": "yinni-header-interceptor",
        "body": [
            "import 'dart:async';",
            "import 'package:yinni_mobile/features/auth/domain/usecase/auth_usecase.dart';",
            "import 'package:chopper/chopper.dart';",
            "",
            "class HeaderInterceptor implements Interceptor {",
            "\tfinal AuthUsecase authUsecase;",
            "",
            "\tHeaderInterceptor(this.authUsecase);",
            "",
            "\t@override",
            "\tFutureOr<Response<BodyType>> intercept<BodyType>(Chain<BodyType> chain) async {",
            "\t\tfinal Request original = chain.request;",
            "",
            "\t\tfinal Request updated = await authUsecase.addAuthHeaderInterceptor(original);",
            "\t\tfinal Response<BodyType> response = await chain.proceed(updated);",
            "",
            "\t\treturn response;",
            "\t}",
            "}",
            ""
        ],
    },

    "yinni-json-converter": {
        "prefix": "yinni-json-converter",
        "body": [
            "import 'dart:async';",
            "",
            "import 'package:chopper/chopper.dart';",
            "import 'package:flutter/foundation.dart';",
            "",
            "typedef JsonFactory<T> = T Function(Map<String, dynamic> json);",
            "",
            "class JsonModelConverter extends JsonConverter {",
            "\tconst JsonModelConverter(this.factories);",
            "",
            "\tfinal Map<Type, JsonFactory> factories;",
            "",
            "\tT? _decodeMap<T>(Map<String, dynamic> values) {",
            "\t\tfinal jsonFactory = factories[T];",
            "\t\tif (jsonFactory == null || jsonFactory is! JsonFactory<T>) {",
            "\t\t\treturn null;",
            "\t\t}",
            "\t\treturn jsonFactory(values);",
            "\t}",
            "",
            "\tList<T> _decodeList<T>(Iterable values) =>",
            "\t\t\tvalues.where((v) => v != null).map<T>((v) => _decode<T>(v)).toList();",
            "",
            "\tdynamic _decode<T>(entity) {",
            "\t\tif (entity is Iterable) return _decodeList<T>(entity as List);",
            "\t\tif (entity is Map<String, dynamic>) {",
            "\t\t\treturn _decodeMap<T>(entity);",
            "\t\t}",
            "\t\treturn entity;",
            "\t}",
            "",
            "\t@override",
            "\tFutureOr<Response<ResultType>> convertResponse<ResultType, Item>(",
            "\t\tResponse response,",
            "\t) async {",
            "\t\tdebugPrint('================ RAW BACKEND (PRE-DECODE) ================');",
            "\t\tdebugPrint('Status: ${response.statusCode}');",
            "\t\tdebugPrint('Raw bodyString: ${response.bodyString}');",
            "\t\tdebugPrint('==========================================================');",
            "\t\tfinal jsonRes = await super.convertResponse(response);",
            "\t\treturn jsonRes.copyWith<ResultType>(body: _decode<Item>(jsonRes.body));",
            "\t}",
            "}"
        ],
        "description": "lib/core/repositories/network/utilities"
    },

    "yinni-service-logging-interceptor": {
        "prefix": "yinni-service-logging-interceptor",
        "body": [
            "import 'dart:async';",
            "import 'package:chopper/chopper.dart';",
            "import 'package:flutter/foundation.dart';",
            "import 'package:http/http.dart' as http;",
            "",
            "class ServiceLoggingInterceptor implements Interceptor {",
            "\t@override",
            "\tFutureOr<Response<BodyType>> intercept<BodyType>(Chain<BodyType> chain) async {",
            "\t\tfinal Request request = chain.request;",
            "",
            "\t\t// --- 1. Log Request ---",
            "\t\tdebugPrint('==================== REQUEST START ====================');",
            "\t\tdebugPrint('--> ${request.method} ${request.url}');",
            "\t\tfinal http.BaseRequest baseRequest = await request.toBaseRequest();",
            "\t\tif (baseRequest is http.Request && baseRequest.body.isNotEmpty) {",
            "\t\t\tdebugPrint('Body: ${baseRequest.body}');",
            "\t\t}",
            "\t\tdebugPrint('====================== REQUEST END =====================');",
            "",
            "\t\tfinal Stopwatch stopWatch = Stopwatch()..start();",
            "",
            "\t\ttry {",
            "\t\t\tfinal Response<BodyType> response = await chain.proceed(request);",
            "\t\t\tstopWatch.stop();",
            "\t\t\treturn response;",
            "\t\t} catch (e) {",
            "\t\t\tstopWatch.stop();",
            "\t\t\tdebugPrint('==================== ERROR START ===================');",
            "\t\t\tdebugPrint('URL: ${request.url}');",
            "\t\t\tdebugPrint('TIME: ${stopWatch.elapsedMilliseconds}ms');",
            "\t\t\t",
            "\t\t\t// Check if it's a Chopper error that contains the response",
            "\t\t\tif (e is Response) {",
            "\t\t\t\tdebugPrint('Status Code: ${e.statusCode}');",
            "\t\t\t\tdebugPrint('Raw Body from Error: ${e.bodyString}');",
            "\t\t\t}",
            "\t\t\t",
            "\t\t\tdebugPrint('ERROR DETAILS: $e');",
            "\t\t\tdebugPrint('==================== ERROR END ========================');",
            "\t\t\trethrow;",
            "\t\t}",
            "\t}",
            "}"
        ],
        "description": "lib/core/repositories/network"
    },

